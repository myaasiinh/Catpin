<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.myaasiinh.catpin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_catpin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_catpin"
        android:supportsRtl="true"
        android:theme="@style/Theme.Catatanpintar">

        <activity android:name=".ui.main.view.MainActivity"
            android:resizeableActivity="false"
            tools:ignore="NonResizeableActivity"
            tools:targetApi="n" />

        <activity android:name=".ui.main.view.LainnyaActivity"
            android:resizeableActivity="false"
            tools:ignore="NonResizeableActivity"
            tools:targetApi="n" />

        <activity
            android:name=".ui.main.view.TambahkanmemoActivity"
            android:resizeableActivity="false"
            tools:ignore="NonResizeableActivity"
            tools:targetApi="n" />

        <activity
            android:name=".ui.main.view.TentangAplikasiActivity"
            android:resizeableActivity="false"
            tools:ignore="NonResizeableActivity"
            tools:targetApi="n" />

        <activity
            android:name="com.myaasiinh.catpin.ui.main.view.SplashscreenActivity"
            android:exported="true"
            android:resizeableActivity="false"
            tools:ignore="NonResizeableActivity"
            tools:targetApi="n">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>

